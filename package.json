{
  "name": "ethdoc",
  "description": " A command line tool that generates Smart Contract documentation based off of your solidity file.",
  "version": "0.0.0",
  "author": " @iMuzz",
  "bin": {
    "ethdoc": "./bin/run"
  },
  "bugs": "https://github.com/iMuzz/ethdoc/issues",
  "dependencies": {
    "@0xproject/sol-compiler": "^1.0.4",
    "@oclif/command": "^1.4.36",
    "@oclif/config": "^1.6.33",
    "@oclif/plugin-help": "^2.0.5"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-oclif": "^1.5.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/iMuzz/ethdoc",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "ethdoc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "iMuzz/ethdoc",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
